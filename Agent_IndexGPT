# Sort alphabetically and add numbering
sorted_agents = sorted(gpt_agents)
md_lines = ["# GPT Agent Index\n",
            "A numbered, alphabetical list of custom GPTs curated by TJTM (Theresa Janette Thurmond Morris).\n",
            "| Number | Agent Name |\n|--------|-------------|"]

for i, name in enumerate(sorted_agents, 1):
    md_lines.append(f"| {i} | {name} |")

# Join all lines into a single Markdown-formatted string
markdown_content = "\n".join(md_lines)

